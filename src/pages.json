{
  "pages": [ //pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
    {
      "path": "pages/tabbar/index/index",
      "style": {
        "enablePullDownRefresh": false
      }

    }, {
      "path": "pages/tabbar/my/my",
      "style": {
        "navigationBarTitleText": "我的",
        "enablePullDownRefresh": false
      }

    }, {
      "path": "pages/tabbar/work/work",
      "style": {
        "navigationBarTitleText": "工作台",
        "enablePullDownRefresh": false
      }

    }, {
      "path": "pages/index/index",
      "style": {
        "navigationBarTitleText": "首页"
      }
    }, {
      "path": "pages/test/test",
      "style": {
        "navigationBarTitleText": "测试",
        "enablePullDownRefresh": false
      }

    }, {
      "path": "pages/login/login",
      "style": {
        "navigationBarTitleText": "登录",
        "enablePullDownRefresh": false
      }

    }
  ],
  "subPackages": [{
    "root": "pages/subIndex",
    "pages": [{
      "path": "list/list",
      "style": {
        "navigationBarTitleText": "分包list",
        "enablePullDownRefresh": false
      }
    }]
  }],
  "globalStyle": {
    "navigationBarTextStyle": "white",
    "navigationBarTitleText": "食堂管理端",
    "navigationBarBackgroundColor": "#0088FF",
    "backgroundColor": "#fff"
  },
  "tabBar": {
    "color": "#333",
    "selectedColor": "#0088FF",
    "backgroundColor": "#F6F6F6",
    "list": [{
        "pagePath": "pages/tabbar/index/index",
        "text": "简报",
        "iconPath": "static/images/tabbar/index.png",
        "selectedIconPath": "static/images/tabbar/indexSelect.png"
      },
      {
        "pagePath": "pages/tabbar/work/work",
        "text": "工作台",
        "iconPath": "static/images/tabbar/work.png",
        "selectedIconPath": "static/images/tabbar/workSelect.png"
      },
      {
        "pagePath": "pages/tabbar/my/my",
        "text": "我的",
        "iconPath": "static/images/tabbar/my.png",
        "selectedIconPath": "static/images/tabbar/mySelect.png"
      }
    ]
  },
  "easycom": {
    "autoscan": true, // 是否自动扫描组件
    "custom": { // 自定义扫描规则
      "^uni-(.*)": "@dcloudio/uni-ui/lib/uni-$1/uni-$1.vue",
      "^ucharts-(.*)": "@/components/ucharts/ucharts-$1/ucharts-$1.vue"
    }
  }
}
